               <placeName xml:id="name000048ga-5" type="building-name" xml:lang="zh-latn-wadegile">Tai-feng-hsüan</placeName>
               <idno type="URI">https://architecturasinica.org/place/000048ga</idno>
               <state type="existence" from="1718-01-01" to="1718-12-31">
