               <placeName xml:id="name000048u-5" type="building-name" xml:lang="zh-latn-wadegile">Kung-shu-tzu-tz`u</placeName>
               <idno type="URI">https://architecturasinica.org/place/000048u</idno>
               <state type="existence" from="1700-01-01" to="1800-01-01">
