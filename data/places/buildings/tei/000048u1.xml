         <listPlace>
            <place xml:id="place-000048u" type="schema:LandmarksOrHistoricalBuildings">
               <placeName xml:id="name000048u-1" type="building-name" xml:lang="en">Gong Shuzi Shrine Complex</placeName>
               <placeName xml:id="name000048u-2" type="building-name" xml:lang="zh-Hant">公輸子祠</placeName>
               <placeName xml:id="name000048u-3" type="building-name" xml:lang="zh-Hans">公输子祠</placeName>
